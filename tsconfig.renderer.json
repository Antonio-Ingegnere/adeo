{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES2017",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ES2020", "DOM"],
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src/renderer/**/*.ts", "src/types.ts", "src/renderer/**/*"]
  ,
  "exclude": []
}
